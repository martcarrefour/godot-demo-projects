; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pet"
run/main_scene="res://Game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Events="*res://core/events.gd"
Globals="*res://core/globals.gd"
AudioManager="*res://core/audio_manager.gd"
CoursorManger="*res://core/coursor_manger.tscn"

[display]

window/size/viewport_width=144
window/size/viewport_height=256
window/size/window_width_override=768
window/size/window_height_override=1365
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/gdscript_formatter/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
